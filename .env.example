# Database
DATABASE_URL=postgresql://yuvgo:yuvgo_password@localhost:5432/yuvgo_db

# Redis
REDIS_URL=redis://localhost:6379

# JWT
JWT_SECRET=your-secret-key-change-in-production
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7

# Environment
ENVIRONMENT=development

# Payment Providers
PAYME_MERCHANT_ID=your-payme-merchant-id
PAYME_SECRET_KEY=your-payme-secret-key
CLICK_MERCHANT_ID=your-click-merchant-id
CLICK_SECRET_KEY=your-click-secret-key
PAYNET_MERCHANT_ID=your-paynet-merchant-id
PAYNET_SECRET_KEY=your-paynet-secret-key

# IpakYuli Bank E-Comm Payment Gateway
IPAKYULI_BASE_URL=https://partner.ecomm.staging.ipakyulibank.uz
IPAKYULI_ACCESS_TOKEN=your-access-token
IPAKYULI_CASHBOX_ID=your-cashbox-id
IPAKYULI_MERCHANT_ID=your-merchant-id
IPAKYULI_LOGIN=your-login
IPAKYULI_PASSWORD=your-password
PAYMENT_SUCCESS_URL=https://yuvgo.uz/payment/success
PAYMENT_FAIL_URL=https://yuvgo.uz/payment/fail

# Firebase
FIREBASE_CREDENTIALS_PATH=./firebase-credentials.json

# SMS Provider
SMS_PROVIDER_API_KEY=your-sms-provider-api-key
SMS_PROVIDER_URL=https://sms-provider.uz/api

# QR Settings
QR_TOKEN_TTL_SECONDS=120
VISIT_COOLDOWN_HOURS=4

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60
